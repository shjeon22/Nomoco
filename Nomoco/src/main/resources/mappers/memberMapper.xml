<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
  
  <mapper namespace="com.nmc.mapper.memberMapper">
  
  <insert id="insertMember">
  insert into member
  (id,pw,name,tel,email,address)
   values(#{id},#{pw},#{name},#{tel},#{email},#{address})
   </insert>
   
   <select id="login" resultType="com.nmc.domain.MemberVO">
   select*from member
   where
   id=#{id} and pw=#{pw}
   </select>
  
  </mapper>